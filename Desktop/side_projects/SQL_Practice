SELECT name,imdb_rating FROM movies;

SELECT DISTINCT genre FROM movies;

SELECT * FROM movies WHERE imdb_rating >8;

SELECT * FROM movies WHERE name LIKE 'Se_en';

SELECT * FROM movies WHERE name LIKE 'a%';
